@{
    Layout = "~/Views/Shared/_ReportLayout.cshtml";
    bool showDesc = false;
    if (Request.Params["showDesc"] != null) {
        if(Request.Params["showDesc"] == "1"){
            showDesc = true;
        }
    }
}

<div class="crm-print-block">
    @foreach (var system in ViewBag.Systems)
    {
        <h2>@system</h2>
        
        <table class="table table-striped table-bordered contract-supervision-table">
            <th>Customer</th>
            <th>Contract id</th>
            <th style="width: 15em">Module</th>
            <th>Representative</th>
            <th>System</th>
            <th>Classification</th>
            @foreach (Dictionary<string, object> customerModule in ViewBag.Customermodules)
            {
                <tr class="" style="page-break-inside:avoid">
                    @foreach (KeyValuePair<string, object> dic in customerModule)
                    {
                        <td>@dic.Value</td>
                    }
                </tr>
            }
        </table>
    }
</div>


